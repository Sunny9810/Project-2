<div class="optionpg">

  <div class="previous-workout card" id="previous-workout">
    <div class="card-body">
      <h2 class="card-title">Previous Workout</h2>
      <p class="card-text">{{previousWorkout}}</p>
    </div>
  </div>
  <br />
  <div class="warm-up card" id="warm-up">
    <div class="card-body">
      <h2 class="card-title">Warm-Up</h2>
      <p>{{warmUp}}</p>
    </div>
  </div>

  <div class="todays-workout card" id="todays-workout">
    <div class="card-body">
      <h2 class="card-title">Today's Workout</h2>
      <p class="card-text">{{todaysWorkout}}</p>
    </div>
  </div>
  <div class="cool-down card" id="cool-down">
    <div class="card-body">
      <h2 class="card-title">Cool Down</h2>
      <p class="card-text">{{coolDown}}</p>
    </div>
  </div>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe"
    crossorigin="anonymous"
  ></script>
  <script src="/js/optionpg.js"></script>
</div>