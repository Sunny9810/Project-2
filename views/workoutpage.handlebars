<div class="workoutpage">
  <div class="workout-container">
      <h1 class="row" id="form-color">
           Choose which muscle group you would like to wokrout today!
        </h1>
   </div>

    {{!-- {{#if musclegroup}}
    <p>you chose {{muscle group name}}, here is today's workout {{muscle groupexercise}}</p>
    {{/if}} --}}

<!-- Modal; will appear when button for exercise is clicked-->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Today's Workout:</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save Workout</button>
      </div>
    </div>
  </div>
</div>

{{!-- carousel --}}

